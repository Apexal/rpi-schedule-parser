{
  "name": "rpi-schedule-parser",
  "version": "0.1.0",
  "description": "A simple parser that can convert CRNs into objects detailing the courses and periods.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json"
  },
  "keywords": [],
  "author": "Frank Matranga <matraf@rpi.edu>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/jsdom": "^16.2.3",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "jsdom": "^16.2.2"
  }
}