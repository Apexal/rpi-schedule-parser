{
  "name": "rpi-schedule-parser",
  "version": "0.1.0",
  "description": "A simple parser that can convert CRNs into objects detailing the courses and periods.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib /**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npmrun build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Frank Matranga <matraf@rpi.edu>",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  }
}